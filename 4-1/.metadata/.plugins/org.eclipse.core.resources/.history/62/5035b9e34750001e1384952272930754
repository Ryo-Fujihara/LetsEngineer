package check;

import constants.Constants;

public class Main {
	
	private String firstName = "藤原";
	private String lastName = "涼";
	
	
	private static void printName (String myoji,String namae) {
		
		System.out.println("printNameメソッド→ "  + myoji + namae);
	}

	public static void main(String [] args) {
		
		Main pn = new printName(myoji,namae);
		pn.printName(firstName,lastName);
		
		Pet pt = new Pet(Constants.CHECK_CLASS_JAVA, Constants.CHECK_CLASS_HOGE) ;
		pt.introduce();
			
		
		RobotPet rp = new RobotPet(Constants.CHECK_CLASS_R2D2,Constants.CHECK_CLASS_LUKE);
		rp.introduce();
		
		
		
	}
	
}
